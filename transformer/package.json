{
  "name": "fullbay-denormalized",
  "version": "1.0.0",
  "description": "Fullbay database denormalization and JSON export tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --max-old-space-size=8192 dist/index.js",
    "gen": "node scripts/run-with-tunnel.js",
    "server": "node scripts/server-with-tunnel.js",
    "remote-fetch": "node --max-old-space-size=8192 -r ts-node/register src/remote-fetch.ts",
    "test": "jest",
    "export": "npm run build && npm run start",
    "autocare:parquet": "node scripts/build-parquet.js --out ../output"
  },
  "keywords": [
    "fullbay",
    "denormalize",
    "export",
    "json"
  ],
  "author": "Fullbay Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@duckdb/node-api": "1.3.2-alpha.26",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "mysql2": "^3.6.5",
    "tunnel-ssh": "^5.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/yargs": "^17.0.32",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
